{
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^20.0.1",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "@types/node": "^20.11.23",
    "cypress": "^13.6.6",
    "esbuild": "^0.20.1",
    "typescript": "^5.3.3"
  },
  "cypress-cucumber-preprocessor": {
    "filterSpecs": true,
    "omitFiltered": true,
    "json": {
      "enabled": true,
      "output": "reports/cucumber-report.json"
    },
    "html": {
      "enabled": true,
      "output": "reports/cucumber-report.html"
    }
  }
}
